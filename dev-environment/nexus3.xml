<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Date>2019-2-19</Date>
  <Beta>False</Beta>
  <Category>SoftwareDevelopment:CD SoftwareDevelopment:CI</Category>
  <Name>Nexus</Name>
  <Project>https://sonatype.com/</Project>
  <Changes>
    [font size=3]2019.02.10[/font]
    - Initial Release
  </Changes>
  <Overview>
    Sonatype Nexus3 repository
  </Overview>
  <Description>
    Store and distribute Maven/Java, npm, NuGet, RubyGems, Docker, P2, OBR, APT and YUM and more.[br][br]
    [b][span style='color: #E80000;']Directions:[/span][/b][br]
    [b]/sonatype-work[/b] : this is where nexus will store it's databases and configuration.[br][br]
  </Description>
  <Registry>https://hub.docker.com/r/sonatype/nexus</Registry>
  <GitHub>https://github.com/sonatype/nexus3</GitHub>
  <Repository>sonatype/nexus3:latest</Repository>
  <Support>https://issues.sonatype.org/projects/NEXUS</Support>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Networking>
    <Publish>
      <Port>
        <HostPort>8181</HostPort>
        <ContainerPort>8081</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>NEXUS_CONTEXT</Name>
      <Value>nexus</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/development/nexus3-data</HostDir>
      <ContainerDir>/opt/sonatype/nexus</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:8081]/nexus</WebUI>
  <Banner></Banner>
  <Icon>https://s3.amazonaws.com/cntech.io/logos/nexus.png</Icon>
</Containers>
