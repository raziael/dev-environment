<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Date>2019-2-10</Date>
  <Beta>False</Beta>
  <Category>SoftwareDevelopment:CD SoftwareDevelopment:CI</Category>
  <Name>Jenkins</Name>
  <Project>https://jenkins.io/</Project>
  <Changes>
      [font size=3]2019.02.10[/font]
      - Initial Release
  </Changes>
  <Overview>
    The Jenkins Continuous Integration and Delivery server
  </Overview>
  <Description>
    The Jenkins Continuous Integration and Delivery server[br][br]
    [b][span style='color: #E80000;']Directions:[/span][/b][br]
    [b]/var/jenkins_home[/b] : this is where all jenkins data resides.[br][br]
  </Description>
  <Registry>https://hub.docker.com/_/jenkins</Registry>
  <GitHub>https://github.com/jenkinsci/jenkins</GitHub>
  <Repository>jenkins/jenkins:lts</Repository>
  <Support>https://issues.jenkins-ci.org/secure/Dashboard.jspa</Support>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8082</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>JENKINS_OPTS</Name>
      <Value>--prefix=/jenkins</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/development/jenkins/home</HostDir>
      <ContainerDir>/var/jenkins_home</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/var/run/docker.sock</HostDir>
      <ContainerDir>/var/run/docker.sock</ContainerDir>
      <Mode>ro</Mode>
    </Volume>
    <Volume>
      <HostDir>/usr/bin/docker</HostDir>
      <ContainerDir>/usr/bin/docker</ContainerDir>
      <Mode>ro</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:8082]/jenkins</WebUI>
  <Banner></Banner>
  <Icon>https://s3.amazonaws.com/cntech.io/logos/jenkins.png</Icon>
</Containers>
