<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Date>2019-2-10</Date>
  <Beta>False</Beta>
  <Category>SoftwareDevelopment:CD SoftwareDevelopment:CI</Category>
  <Name>NginX</Name>
  <Project>http://nginx.com/</Project>
  <Changes>
[font size=3]2019.02.10[/font]
- Initial Release
  </Changes>
  <Overview>
   NginX
  </Overview>
  <Description>
    Nginx
  </Description>
  <Registry>https://hub.docker.com/_/nginx</Registry>
  <GitHub>https://github.com/nginx</GitHub>
  <Repository>nginx</Repository>
  <Support>https://issues.nginx</Support>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8089</HostPort>
        <ContainerPort>8089</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment/>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/development/nginx/nginx.conf</HostDir>
      <ContainerDir>/etc/nginx/nginx.conf</ContainerDir>
      <Mode>ro</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:8980]/</WebUI>
  <Banner></Banner>
  <Icon>https://s3.amazonaws.com/cntech.io/logos/nginx.png</Icon>
</Containers>
