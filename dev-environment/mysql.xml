<?xml version="1.0" encoding="utf-8"?>
<Container>
	<Beta>False</Beta>
	<Category>Productivity: Tools</Category>
	<Name>MySQL</Name>
	<Overview>
    MySQL strives to be the logical choice for database professionals looking for a robust, scalable, and reliable SQL server.
  </Overview>
	<Description>
    MySQL strives to be the logical choice for database professionals looking for a robust, scalable, and reliable SQL server.
  </Description>
	<Registry>https://hub.docker.com/_/mysql</Registry>
	<GitHub>http://github.com/mysql</GitHub>
	<Repository>mysql</Repository>
	<Support>http://mysql.com</Support>
	<BindTime>true</BindTime>
	<Privileged>false</Privileged>
	<Networking>
		<Mode>bridge</Mode>
		<Publish>
			<Port>
				<HostPort>3306</HostPort>
				<ContainerPort>3306</ContainerPort>
				<Protocol>tcp</Protocol>
			</Port>
		</Publish>
	</Networking>
	<Environment>
		<Variable>
			<Name>MYSQL_ROOT_PASSWORD_FILE</Name>
			<Value>/var/lib/mysql/MYSQL_ROOT_PASSWORD</Value>
		</Variable>
	</Environment>
	<Data>
		<Volume>
			<HostDir>/mnt/user/appdata/development/mysql/data</HostDir>
			<ContainerDir>/var/lib/mysql</ContainerDir>
			<Mode>rw</Mode>
		</Volume>
	</Data>
	<WebUI>http://[IP]:[PORT:8980]/</WebUI>
	<Banner></Banner>
	<Icon>https://s3.amazonaws.com/cntech.io/logos/mysql.png</Icon>
</Container>