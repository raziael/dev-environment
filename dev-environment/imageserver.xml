<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Date>2019-2-10</Date>
  <Beta>False</Beta>
  <Category>Centally:Core</Category>
  <Name>Users API</Name>
  <Project>https://centally.com/</Project>
  <Changes>
      [font size=3]2019.03.24[/font]
      - Initial Release
  </Changes>
  <Overview>
    Image Server
  </Overview>
  <Description>
    The imageserver is part of Core Stack[br][br]
  </Description>
  <Registry>http://tower:5000/raziael/imageserver</Registry>
  <GitHub>https://github.com/raziael</GitHub>
  <Repository>tower:5000/raziael/imageserver:latest</Repository>
  <Support>https://centally.com</Support>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8082</HostPort>
        <ContainerPort>8083</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <WebUI>http://[IP]:[PORT]/v1/images</WebUI>
  <Banner></Banner>
